{
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "vueIndentScriptAndStyle": true,
  "htmlWhitespaceSensitivity": "ignore",
  "singleAttributePerLine": false,
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "plugins": ["prettier-plugin-tailwindcss"]
}
